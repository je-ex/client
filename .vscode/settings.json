{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "package-lock.json": true,
        "LICENSE": true,
        ".gitattributes": true,
        ".gitignore": true,
        ".npmignore": true,
        "changes": true,
        "README.md": true,
        ".vscode": true,
        "jest.config.js": true
    },
    "hide-files.files": [
        "package-lock.json",
        "LICENSE",
        ".gitattributes",
        ".gitignore",
        ".npmignore",
        "changes",
        "README.md",
        ".vscode",
        "jest.config.js"
    ],
    "material-icon-theme.folders.associations": {
        "media" : "Middleware"
    },

    "editor.tokenColorCustomizations": {
        "comments": "#999898"
    },

    "colorful-comments.tags":
    [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "?",
            "color": "#0076FF",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "//",
            "color": "#999898",
            "strikethrough": true,
            "backgroundColor": "transparent"
        },
        {
            "tag": "^",
            "color": "#EAF622",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "*",
            "color": "#28FF00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "&",
            "color": "#FF06A0",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "~",
            "color": "#BE00FF",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": " [",
            "color": "#E16285",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "-",
            "color": "#988A93",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "/* ┌",
            "color": "#676767",
        },
        {
            "tag": "/* └",
            "color": "#676767",
        },
    ],
    "compile-hero.disable-compile-files-on-did-save-code": true
}